<div class="govuk-form-group postcode-lookup__search js-enabled">
    <label class="govuk-label" for="postcode-lookup">Postcode lookup</label>
    <input autocomplete="off" type="text" id="postcode-lookup" class="postcode-input govuk-input js-PostcodeLookup__query govuk-input--width-5">
    <a href="#" id="find_uk_address" class="postcode-button govuk-button js-PostcodeLookup__search-btn" role="button">Find UK address</a>
</div>

<div class="govuk-form-group postcode-lookup__results">
    <label class="govuk-label" for="address-search-result">Address</label>
    <select class="govuk-select js-PostcodeLookup__search-results" id="address-search-result">
        <option value="">Please select an address...</option>
    </select>
</div>

<div class="postcode-lookup__error govuk-warning-text__text">
</div>
