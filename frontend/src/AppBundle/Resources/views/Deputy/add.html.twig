{% extends 'AppBundle:Layouts:base-template.html.twig' %}
{% form_theme form 'AppBundle:Form:govuk_frontend.html.twig' %}

{% block htmlTitle %}Deputy{% endblock %}
{% block pageTitle %}Deputy{% endblock %}

{% block content %}
	deputy add

	<hr/>ORDER: id={{ order.id }} |{{ order.subType }} | {{ order.hasAssetsAboveThreshold }}
	<br/>Types:
	{% for t in order.types %}
		{{ t.type }} (id={{ t.id }}),
	{% endfor %}
        {{ client.caseNumber }} |
        {{ client.clientName }}

        {{ form_start(form) }}
        {{ form_widget(form) }}
        {{ form_end(form) }}

{% endblock %}
